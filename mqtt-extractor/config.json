{
  "name": "MQTT to Cognite Data Fusion Extractor",
  "version": "0.9.0",
  "slug": "mqtt-extractor",
  "description": "Extracts MQTT messages and pushes them to Cognite Data Fusion (CDF)",
  "arch": ["amd64", "aarch64", "armv7"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "options": {
    "mqtt_hostname": "homeassistant.local",
    "mqtt_port": 1883,
    "mqtt_username": "",
    "mqtt_password": "",
    "mqtt_client_id": "mqtt-extractor",
    "mqtt_clean_session": false,
    "mqtt_topics": ["*"],
    "mqtt_raw_topics": [],
    "mqtt_event_topics": [],
    "mqtt_qos": 1,
    "cdf_url": "",
    "cdf_project": "",
    "idp_client_id": "",
    "idp_token_url": "",
    "idp_client_secret": "",
    "idp_scopes": "",
    "external_id_prefix": "mqtt:",
    "upload_interval": 1,
    "create_missing_timeseries": true,
    "timeseries_view_external_id": "haTimeSeries",
    "enable_data_model": false,
    "instance_space": "",
    "data_model_space": "sp_enterprise_schema_space",
    "data_model_version": "v1",
    "source_system_space": "cdf_cdm",
    "source_system_version": "v1",
    "trigger_workflows_on_raw_change": false,
    "workflow_external_id": "",
    "workflow_version": "",
    "workflow_min_trigger_interval": 300,
    "workflow_debounce_seconds": 5,
    "enable_alarm_events": false,
    "alarm_event_instance_space": "",
    "alarm_event_view_external_id": "haAlarmEvent",
    "alarm_event_data_model_space": "sp_enterprise_schema_space",
    "alarm_event_data_model_version": "v1",
    "alarm_event_source_system": "MQTT",
    "data_model_writes": [],
    "log_level": "INFO"
  },
  "schema": {
    "mqtt_hostname": "str",
    "mqtt_port": "int",
    "mqtt_username": "str?",
    "mqtt_password": "str?",
    "mqtt_client_id": "str?",
    "mqtt_clean_session": "bool?",
    "mqtt_topics": ["str"],
    "mqtt_raw_topics": ["str"],
    "mqtt_event_topics": ["str"],
    "mqtt_qos": "int(0,2)?",
    "cdf_url": "str",
    "cdf_project": "str",
    "idp_client_id": "str",
    "idp_token_url": "str",
    "idp_client_secret": "str",
    "idp_scopes": "str",
    "external_id_prefix": "str?",
    "upload_interval": "int(1,)?",
    "create_missing_timeseries": "bool?",
    "timeseries_view_external_id": "str?",
    "enable_data_model": "bool?",
    "instance_space": "str?",
    "data_model_space": "str?",
    "data_model_version": "str?",
    "source_system_space": "str?",
    "source_system_version": "str?",
    "trigger_workflows_on_raw_change": "bool?",
    "workflow_external_id": "str?",
    "workflow_version": "str?",
    "workflow_min_trigger_interval": "int(1,)?",
    "workflow_debounce_seconds": "int(1,)?",
    "enable_alarm_events": "bool?",
    "alarm_event_instance_space": "str?",
    "alarm_event_view_external_id": "str?",
    "alarm_event_data_model_space": "str?",
    "alarm_event_data_model_version": "str?",
    "alarm_event_source_system": "str?",
    "data_model_writes": ["list(topic str, view_external_id str, instance_space str, data_model_space str?, data_model_version str?)?"],
    "log_level": "match(^(DEBUG|INFO|WARNING|ERROR)$)?"
  },
  "ports": {},
  "ports_description": {},
  "host_network": false
}

